{
  "name": "action-clean-up-artifacts",
  "private": true,
  "version": "1.0.1",
  "description": "Cleans up artifacts for the specified workflow run",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "ncc build --minify",
    "test": "exit 0",
    "all": "npm run build && npm run lint:fix && npm run package && npm test"
  },
  "keywords": [],
  "author": "Volodymyr Kolesnykov <volodymyr@wildwolf.name> (https://wildwolf.name/)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^2.5.4",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
