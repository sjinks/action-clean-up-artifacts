{
  "name": "action-clean-up-artifacts",
  "private": true,
  "version": "2.0.0",
  "description": "Cleans up artifacts for the specified workflow run",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "ncc build --minify",
    "test": "exit 0",
    "all": "npm run build && npm run lint:fix && npm run package && npm test"
  },
  "keywords": [],
  "author": "Volodymyr Kolesnykov <volodymyr@wildwolf.name> (https://wildwolf.name/)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^3.0.0",
    "@types/node": "^24.0.0",
    "@vercel/ncc": "^0.38.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": "24"
  }
}
